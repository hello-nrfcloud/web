{
  "name": "@nordicsemiconductor/nrf-guide-web",
  "version": "0.0.0-development",
  "description": "A getting started guide for the Nordic Semiconductor Development Kits",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/nrf.guide.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/nrf.guide/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/nrf.guide#readme",
  "keywords": [
    "nordicsemiconductor",
    "cellular-iot",
    "nrf-guide"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "scripts": {
    "start": "vite",
    "build": "node --max_old_space_size=8192 ./node_modules/vite/bin/vite.js build --emptyOutDir",
    "prepare": "husky install",
    "test": "jest"
  },
  "dependencies": {
    "date-fns": "2.29.3",
    "lucide-preact": "0.109.0",
    "preact": "10.11.3",
    "styled-components": "6.0.0-beta.9"
  },
  "devDependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "11.0.174",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "11.0.2",
    "@nordicsemiconductor/from-env": "1.0.3",
    "@preact/preset-vite": "2.5.0",
    "@swc/core": "1.3.32",
    "@swc/jest": "0.2.24",
    "@types/jest": "29.4.0",
    "@types/node": "18.11.18",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.50.0",
    "aws-cdk": "2.63.0",
    "aws-cdk-lib": "2.63.0",
    "bootstrap": "5.2.3",
    "cdk": "2.63.0",
    "chalk": "5.2.0",
    "eslint": "8.33.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-unicorn": "45.0.2",
    "husky": "8.0.3",
    "jest": "29.4.1",
    "lint-staged": "13.1.0",
    "preact-render-to-string": "5.2.6",
    "prettier": "2.8.3",
    "prettier-plugin-organize-imports": "3.2.2",
    "regenerator-runtime": "0.13.11",
    "rehype-format": "4.0.1",
    "rehype-stringify": "9.0.3",
    "remark": "14.0.2",
    "remark-extract-frontmatter": "3.2.0",
    "remark-frontmatter": "4.0.1",
    "remark-rehype": "10.1.0",
    "tsx": "3.12.2",
    "typescript": "4.9.5",
    "vite-plugin-ssr": "0.4.74",
    "yaml": "2.2.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts,.jsx,.tsx"
    ],
    "*.{md,json,yaml,yml,html}": [
      "prettier --write"
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "testRegex": ".+\\.spec\\.ts$",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ]
  }
}
