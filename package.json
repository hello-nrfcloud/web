{
  "name": "@hello.nrfcloud.com/web",
  "version": "0.0.0-development",
  "description": "Retrieve real-time data from your long-range Nordic Semiconductor Development Kit within seconds",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hello-nrfcloud/web.git"
  },
  "bugs": {
    "url": "https://github.com/hello-nrfcloud/web/issues"
  },
  "homepage": "https://hello.nrfcloud.com",
  "keywords": [
    "nordicsemiconductor",
    "cellular-iot",
    "hello-nrfcloud"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "scripts": {
    "start": "vite",
    "build": "node --max_old_space_size=8192 ./node_modules/vite/bin/vite.js build --emptyOutDir && cp -v ./manifest.json ./build/client/",
    "prepare": "husky install",
    "test": "find ./ -type f -name \\*.spec.ts -not -path './e2e-tests/*' -not -path './node_modules/*' | xargs npx tsx --test --test-reporter spec",
    "test:e2e": "npx playwright test e2e-tests/",
    "postinstall": "cp -r node_modules/svg-country-flags/svg/ static/flags"
  },
  "dependencies": {
    "@hello.nrfcloud.com/proto": "5.5.13",
    "@sentry/browser": "7.73.0",
    "@sinclair/typebox": "0.31.17",
    "classnames": "2.3.2",
    "code-128-encoder": "3.1.1",
    "compare-versions": "6.1.0",
    "date-fns": "2.30.0",
    "e118-iin-list": "3.1.0",
    "html5-qrcode": "2.3.8",
    "lodash-es": "4.17.21",
    "lucide-preact": "0.285.0",
    "maplibre-gl": "3.4.1",
    "preact": "10.18.1",
    "qrcode": "1.5.3",
    "semver": "7.5.4",
    "svg-country-flags": "1.2.10"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudformation": "3.427.0",
    "@aws-sdk/client-cloudfront": "3.427.0",
    "@aws-sdk/client-iam": "3.427.0",
    "@babel/plugin-syntax-import-assertions": "7.22.5",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "12.0.94",
    "@nordicsemiconductor/cloudformation-helpers": "8.0.0",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "16.0.22",
    "@nordicsemiconductor/from-env": "2.0.0",
    "@playwright/test": "1.39.0",
    "@preact/preset-vite": "2.5.0",
    "@swc/core": "1.3.92",
    "@types/lodash-es": "4.17.9",
    "@types/node": "20.8.4",
    "@types/qrcode": "1.5.2",
    "aws-cdk": "2.100.0",
    "aws-cdk-lib": "2.100.0",
    "bootstrap": "5.3.2",
    "cdk": "2.100.0",
    "chalk": "5.3.0",
    "eslint": "8.51.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-unicorn": "48.0.1",
    "glob": "10.3.10",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "playwright": "1.39.0",
    "preact-render-to-string": "6.2.2",
    "prettier": "3.0.3",
    "prettier-plugin-organize-imports": "3.2.3",
    "rehype-format": "5.0.0",
    "rehype-stringify": "10.0.0",
    "remark": "15.0.1",
    "remark-extract-frontmatter": "3.2.0",
    "remark-frontmatter": "5.0.0",
    "remark-rehype": "11.0.0",
    "tsmatchers": "5.0.0",
    "tsx": "3.13.0",
    "typescript": "5.2.2",
    "vike": "0.4.143"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts,.jsx,.tsx"
    ],
    "*.{md,json,yaml,yml,html}": [
      "prettier --write"
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  }
}
