{
  "name": "@hello.nrfcloud.com/web",
  "version": "0.0.0-development",
  "description": "Retrieve real-time data from your long-range Nordic Semiconductor Development Kits within seconds",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hello-nrfcloud/web.git"
  },
  "bugs": {
    "url": "https://github.com/hello-nrfcloud/web/issues"
  },
  "homepage": "https://hello.nrfcloud.com",
  "keywords": [
    "nordicsemiconductor",
    "cellular-iot",
    "hello-nrfcloud"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "scripts": {
    "start": "vite",
    "build": "node --max_old_space_size=8192 ./node_modules/vite/bin/vite.js build --emptyOutDir && node --no-warnings merge-manifest.json.js",
    "prepare": "husky install",
    "test": "jest",
    "test:e2e": "npx playwright test e2e-tests/"
  },
  "dependencies": {
    "@aws-amplify/core": "5.5.1",
    "@aws-sdk/client-cognito-identity": "3.354.0",
    "@aws-sdk/credential-provider-cognito-identity": "3.354.0",
    "@hello.nrfcloud.com/proto": "1.3.3",
    "@sinclair/typebox": "0.28.17",
    "classnames": "2.3.2",
    "date-fns": "2.30.0",
    "html5-qrcode": "2.3.8",
    "lodash-es": "4.17.21",
    "lucide-preact": "0.248.0",
    "maplibre-gl": "3.1.0",
    "preact": "10.15.1",
    "qrcode": "1.5.3",
    "styled-components": "6.0.0-rc.3"
  },
  "devDependencies": {
    "@aws-sdk/client-iam": "3.354.0",
    "@babel/plugin-syntax-import-assertions": "7.22.5",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "12.0.43",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "15.0.10",
    "@nordicsemiconductor/from-env": "2.0.0",
    "@playwright/test": "1.35.1",
    "@preact/preset-vite": "2.5.0",
    "@swc/core": "1.3.66",
    "@swc/jest": "0.2.26",
    "@types/jest": "29.5.2",
    "@types/lodash-es": "4.17.7",
    "@types/node": "20.3.1",
    "@types/qrcode": "1.5.0",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "aws-cdk": "2.84.0",
    "aws-cdk-lib": "2.84.0",
    "bootstrap": "5.3.0",
    "cdk": "2.84.0",
    "chalk": "5.2.0",
    "eslint": "8.43.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-unicorn": "47.0.0",
    "glob": "10.2.7",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jsonpath-plus": "7.2.0",
    "lint-staged": "13.2.2",
    "playwright": "1.35.1",
    "preact-render-to-string": "6.1.0",
    "prettier": "2.8.8",
    "prettier-plugin-organize-imports": "3.2.2",
    "regenerator-runtime": "0.13.11",
    "rehype-format": "4.0.1",
    "rehype-stringify": "9.0.3",
    "remark": "14.0.3",
    "remark-extract-frontmatter": "3.2.0",
    "remark-frontmatter": "4.0.1",
    "remark-rehype": "10.1.0",
    "tsx": "3.12.7",
    "typescript": "5.1.3",
    "vite-plugin-ssr": "0.4.131",
    "xml-js": "1.6.11",
    "yaml": "2.3.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts,.jsx,.tsx"
    ],
    "*.{md,json,yaml,yml,html}": [
      "prettier --write"
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "testRegex": ".+\\.spec\\.ts$",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/e2e-tests/"
    ]
  }
}
