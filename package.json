{
  "name": "@nordicsemiconductor/nrf-guide-web",
  "version": "0.0.0-development",
  "description": "A getting started guide for the Nordic Semiconductor Development Kits",
  "keywords": [
    "nordicsemiconductor",
    "guide",
    "dk"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "scripts": {
    "start": "vite",
    "build": "node --max_old_space_size=8192 ./node_modules/vite/bin/vite.js build --emptyOutDir",
    "prepare": "husky install"
  },
  "dependencies": {
    "date-fns": "2.29.3",
    "lucide-preact": "0.105.0",
    "preact": "10.11.3",
    "styled-components": "5.3.6"
  },
  "devDependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "11.0.172",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "11.0.2",
    "@nordicsemiconductor/from-env": "1.0.3",
    "@preact/preset-vite": "2.5.0",
    "@swc/core": "1.3.27",
    "@types/node": "18.11.18",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "bootstrap": "5.2.3",
    "chalk": "5.2.0",
    "eslint": "8.32.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-unicorn": "45.0.2",
    "handlebars": "4.7.7",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "prettier": "2.8.3",
    "prettier-plugin-organize-imports": "3.2.2",
    "regenerator-runtime": "0.13.11",
    "tsx": "3.12.2",
    "typescript": "4.9.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts,.jsx,.tsx"
    ],
    "*.{md,json,yaml,yml,html}": [
      "prettier --write"
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  }
}
