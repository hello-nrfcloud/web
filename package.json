{
  "name": "@nordicsemiconductor/nrf-guide-web",
  "version": "0.0.0-development",
  "description": "A getting started guide for the Nordic Semiconductor Development Kits",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bifravst/nrf.guide.git"
  },
  "bugs": {
    "url": "https://github.com/bifravst/nrf.guide/issues"
  },
  "homepage": "https://nrf.guide",
  "keywords": [
    "nordicsemiconductor",
    "cellular-iot",
    "nrf-guide"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "scripts": {
    "start": "vite",
    "build": "node --max_old_space_size=8192 ./node_modules/vite/bin/vite.js build --emptyOutDir",
    "prepare": "husky install",
    "test": "jest",
    "test:e2e": "npx playwright test e2e-tests/"
  },
  "dependencies": {
    "date-fns": "2.29.3",
    "html5-qrcode": "2.3.7",
    "lucide-preact": "0.139.0",
    "preact": "10.13.2",
    "styled-components": "6.0.0-beta.11"
  },
  "devDependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "12.0.18",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "13.0.3",
    "@nordicsemiconductor/from-env": "2.0.0",
    "@playwright/test": "1.32.3",
    "@preact/preset-vite": "2.5.0",
    "@swc/core": "1.3.49",
    "@swc/jest": "0.2.24",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "aws-cdk": "2.73.0",
    "aws-cdk-lib": "2.73.0",
    "bootstrap": "5.2.3",
    "cdk": "2.72.1",
    "chalk": "5.2.0",
    "eslint": "8.38.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-unicorn": "46.0.0",
    "glob": "10.0.0",
    "html-entities": "2.3.3",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jsonpath-plus": "7.2.0",
    "lint-staged": "13.2.1",
    "playwright": "1.32.3",
    "preact-render-to-string": "6.0.2",
    "prettier": "2.8.7",
    "prettier-plugin-organize-imports": "3.2.2",
    "regenerator-runtime": "0.13.11",
    "rehype-format": "4.0.1",
    "rehype-stringify": "9.0.3",
    "remark": "14.0.2",
    "remark-extract-frontmatter": "3.2.0",
    "remark-frontmatter": "4.0.1",
    "remark-rehype": "10.1.0",
    "tsx": "3.12.6",
    "typescript": "5.0.4",
    "vite-plugin-ssr": "0.4.112",
    "xml-js": "1.6.11",
    "yaml": "2.2.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts,.jsx,.tsx"
    ],
    "*.{md,json,yaml,yml,html}": [
      "prettier --write"
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "testRegex": ".+\\.spec\\.ts$",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/e2e-tests/"
    ]
  }
}
