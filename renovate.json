{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["github>bifravst/renovate-config"],
  "packageRules": [
    {
      "matchPackagePatterns": [
        "@hello.nrfcloud.com/proto",
        "@sinclair/typebox"
      ],
      "groupName": "proto"
    },
    {
      "matchPackagePatterns": [
        "mcr.microsoft.com/playwright",
        "@playwright/test"
      ],
      "groupName": "playwright"
    }
  ]
}
